<!doctype html>
<html lang="fr">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Financial TicStack</title>
 
  <!--=============== BOXICONS ===============-->
        <link href='https://unpkg.com/boxicons@2.1.2/css/boxicons.min.css' rel='stylesheet'>

        <!--=============== CSS ===============-->
        <link rel="stylesheet" href="/css/styles.css">
        <link rel="stylesheet" href="/css/login.css">
        <link rel="stylesheet" href="/css/income.css">
        <!-- =================Font awesome ===============-->
        
</head>

<body>
 <!-- Here the header and navbar of wep app -->
  <!--===============- Header Financial TicStack ==================-->

     <!--=============== NAV ===============-->
     <div class="nav" id="nav">
      <nav class="nav__content">
          <div class="nav__toggle" id="nav-toggle">
              <i class='bx bx-chevron-right' ></i>
          </div>

          <a href="#" class="nav__logo">
              <img class="nav__logo-image" src="/image/logo.png" alt="logo">
              <span class="nav__logo-name">TicStack</span>
          </a>

          <div class="nav__list">
              <a href="/" class="nav__link active-link">
                  <i class='bx bx-grid-alt'></i>
                  <span class="nav__name">Dashboard</span>
              </a>

              <a href="/login" class="nav__link">
                  <i class='bx bx-file'></i>
                  <span class="nav__name">Income & Expense</span>
              </a>

              <a href="#" class="nav__link">
                  <i class='bx bx-cog' ></i>
                  <span class="nav__name">Settings</span>
              </a>
          </div>
      </nav>
  </div>
    <!----------------- Header Financial TicStack ------------------------------>
    <!--------------------------------------- Main Financial TicStack ------------------------------------>
    <div class="head">
     
       <!--=============== MAIN ===============-->
       <main class="main-container">
        <div class="main-title">
            <h2>DASHBOARD</h2>
          </div>
    
          <div class="main-cards">
    
            <div class="card">
              <div class="card-inner">
                <h3>MEMBERS</h3>
                <i class="bx bx-chevron-right"></i>
              </div>
              <h1 id="countUser"></h1>
            </div>
            <div class="card">
              <div class="card-inner">
                <h3>PAY</h3>
                <i class="bx bx-chevron-right"></i>
              </div>
              <h1 id="payment"></h1>
            </div>
    
            <div class="card">
              <div class="card-inner">
                <h3>WITHDRAWAL</h3>
                <i class="bx bx-chevron-right"></i>
              </div>
              <h1 id="withdrawl"></h1>
            </div>
    
            <div class="card">
              <div class="card-inner">
                <h3>ACCOUNT</h3>
                <i class="bx bx-chevron-right"></i>
              </div>
              <h1 id="id-Account"></h1>
            </div>
    
          </div>    
    
          <div class="composant">
            <div class="ventes">
                <div class="case">
                    <div class="header-case">
                        <h2>List of transactions</h2>
                        <button class="button">See more <span class="fa fa-arrow-right"></span></button>
                    </div>
                    <div class="body-case">
                        <div class="tableau">
                            <table width="100%">
                                <thead>
                                    <tr>
                                        <td>Members</td>
                                        <td>Value</td>
                                        <td>Reason</td>
                                    </tr>
                                </thead>
                                <tbody id="data-user">
                                   
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="stock">
                <div class="case">
                    <div class="header-case">
                        <h2>List of members</h2>
                    </div>
        
                    <div class="body-case">
                        <% for (const user_element of user){ %>
                        <div class="all-users">
                            <div class="infos">
                                <img decoding="async" src="/image/fifa.JPG" width="30" height="30">
                                <div>
                                    <h4> <%= user_element.user_first_name %></h4>
                                    <small><%= user_element.user_name %></small>
                                </div>
                            </div>
        
                            <div class="user-contact">
                                <a href="#<%= user_element.facebook %>">
                                    <span class="fa fa-facebook">F </span>
                                </a>
                                <a href="<%= user_element.email %>">
                                    <span class="fa fa-email"> M</span>
                                </a>
                                <a href="#<%= user_element.contact %>">
                                    <span class="fa fa-phone">P </span>
                                </a>
                            </div>
                        </div>
                        <% } %> 
                    <button class="btn">See more <span class="fa fa-arrow-right"></span></button>
                </div>
            </div>
        </div>
      </main>
    </div>
    
